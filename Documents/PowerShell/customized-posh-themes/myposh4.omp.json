{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "version": 2,
  "final_space": true,
  "blocks": [
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "type": "time",
          "style": "plain",
          "template": "\u250c {{ .CurrentDate | date .Format }} ",
          "properties": {
            "time_format": "15:04:05"
          },
          "background": "",
          "foreground": "#757575",
          "Text": "",
          "Duration": 0,
          "NameLength": 0
        },
        {
          "type": "path",
          "style": "powerline",
          "powerline_symbol": "\ue0b0",
          "template": " {{ .Path }} ",
          "properties": {
            "style": "folder"
          },
          "background": "#91ddff",
          "foreground": "#100e23",
          "Text": "",
          "Duration": 0,
          "NameLength": 0
        },
        {
          "type": "git",
          "style": "powerline",
          "powerline_symbol": "\ue0b0",
          "background_templates": [
            "{{ if or (.Working.Changed) (.Staging.Changed) }}#ff9248{{ end }}",
            "{{ if and (gt .Ahead 0) (gt .Behind 0) }}#f26d50{{ end }}",
            "{{ if gt .Ahead 0 }}#89d1dc{{ end }}",
            "{{ if gt .Behind 0 }}#c5b6ad{{ end }}"
          ],
          "template": " {{ .HEAD }}{{ .BranchStatus }}{{ if .Working.Changed }} {{ .Working.String }}{{ end }}{{ if and (.Working.Changed) (.Staging.Changed) }} \u2502{{ end }}{{ if .Staging.Changed }} {{ .Staging.String }}{{ end }}{{ if gt .StashCount 0 }} {{ .StashCount }}{{ end }} ",
          "properties": {
            "branch_ahead_icon": "\u2191",
            "branch_behind_icon": "\u2193",
            "branch_gone": "\u2262",
            "branch_icon": "\ue0a0 ",
            "branch_identical_icon": "\u2261",
            "cherry_pick_icon": "âœ“ ",
            "commit_icon": "\u25b7 ",
            "fetch_stash_count": true,
            "fetch_status": true,
            "merge_icon": "\u25f4 ",
            "no_commits_icon": "[no commits]",
            "rebase_icon": "\u2c62 ",
            "tag_icon": "\u25b6 "
          },
          "background": "#95ffa4",
          "foreground": "#100e23",
          "Text": "",
          "Duration": 0,
          "NameLength": 0
        },
        {
          "type": "terraform",
          "style": "powerline",
          "powerline_symbol": "\ue0b0",
          "template": " {{ .WorkspaceName }}{{ if .Version }} {{ .Version }}{{ end }} ",
          "background": "#ffee58",
          "foreground": "#100e23",
          "Text": "",
          "Duration": 0,
          "NameLength": 0
        },
        {
          "type": "exit",
          "style": "powerline",
          "powerline_symbol": "\ue0b0",
          "template": " error ",
          "background": "#ff8080",
          "foreground": "#ffffff",
          "Text": "",
          "Duration": 0,
          "NameLength": 0
        }
      ],
      "newline": true
    },
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "type": "text",
          "style": "plain",
          "template": "\u2514",
          "background": "",
          "foreground": "#757575",
          "Text": "",
          "Duration": 0,
          "NameLength": 0
        },
        {
          "type": "text",
          "style": "plain",
          "template": "$",
          "background": "",
          "foreground": "#ffffff",
          "Text": "",
          "Duration": 0,
          "NameLength": 0
        }
      ],
      "newline": true
    }
  ],
  "osc99": true
}
